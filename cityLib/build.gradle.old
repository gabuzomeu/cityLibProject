buildscript {
    repositories {
        mavenCentral()
    }
    dependencies {
        classpath 'com.android.tools.build:gradle:0.5.5'
    }
}

apply plugin: 'android'

apply plugin: 'java'
sourceCompatibility = 1.6
targetCompatibility = 1.6


apply plugin: 'maven'
group = 'eu.ttbox.android.cityLib'
archivesBaseName = 'cityLib'
version = '0.3.5-SNAPSHOT'


ext {
    osmLibVersion = '0.1.2'
    androidCompatibilityVersion = '18'
    slidingmenuVersion = '1.3.1'
    admobVersion = '6.4.1-r11'
    analyticsVersion = '3'
    googlePlayServicesVersion = '9'
    osmdroidVersion = '3.0.10'
    okhttpVersion = '1.1.1'
}

repositories {
    mavenLocal()
    mavenCentral()
}


android {
    compileSdkVersion 18
    buildToolsVersion "18.0.1"

    defaultConfig {
        minSdkVersion 9
        targetSdkVersion 18
    }
}


dependencies {
  compile 'eu.ttbox.android.osm:osm-lib:0.1.2@apklib'

   //  Compatibility
   compile  ('android.support:compatibility-v13:18@jar')
   compile 'android.support:compatibility-v7-appcompat:18@apklib'
   compile 'android.support:compatibility-v7-appcompat:18@jar'
   compile 'com.jeremyfeinstein.slidingmenu:slidingmenu:1.3.1@apklib'

    compile 'com.google.android.admob:admob:6.4.1-r11'
    compile 'com.google.android.analytics:analytics:3'

   // <!-- Play Services -->
     compile 'com.google.android.gms:google-play-services:9@apklib', 'com.google.android.gms:google-play-services:9'

     compile 'org.osmdroid:osmdroid-third-party:3.0.10', 'org.osmdroid:osmdroid-packager:3.0.10'

     compile'com.squareup.okhttp:okhttp:1.1.1'

}
