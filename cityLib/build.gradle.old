buildscript {
    repositories {
        mavenCentral()
    }
    dependencies {
        classpath 'com.android.tools.build:gradle:0.5.6'
    }
}

apply plugin: 'android'
 
apply plugin: 'maven-publish'
apply plugin: 'maven'
group = 'eu.ttbox.android.cityLib'
archivesBaseName = 'cityLib'
version = '0.3.5-SNAPSHOT'



ext {
    osmLibVersion = '0.1.2'
    androidCompatibilityVersion = '18'
    slidingmenuVersion = '1.3.1'
    admobVersion = '6.4.1-r11'
    analyticsVersion = '3'
    googlePlayServicesVersion = '10'
    osmdroidVersion = '3.0.10'
    okhttpVersion = '1.1.1'
}

repositories {
    mavenLocal()
    mavenCentral()
}


android {
    compileSdkVersion 18
    buildToolsVersion "18.0.1"

    defaultConfig {
        minSdkVersion 15
        targetSdkVersion 18
    }
}


 
 
dependencies {
   compile "eu.ttbox.android.osm:osmLib:0.1.2@aar"
   compile "org.slf4j:slf4j-android:1.6.1-RC1@jar"
   
   //  Compatibility 
   compile "com.android.support:support-v13:18.0.+"
   compile "com.android.support:appcompat-v7:18.0.+@aar"

   // https://github.com/jfeinstein10/SlidingMenu/issues/440
    //   compile "com.jeremyfeinstein.slidingmenu:slidingmenu:1.3.2@aar"
   compile "com.jeremyfeinstein.slidingmenu:library:1.3.2@aar"

    compile "com.google.android.admob:admob:6.4.1-r11@jar"
    compile "com.google.android.analytics:analytics:3@jar"

   // <!-- Play Services -->
 	 compile "com.google.android.gms:play-services:3.2.25+@aar"
     compile "org.osmdroid:osmdroid-third-party:3.0.10@jar", "org.osmdroid:osmdroid-packager:3.0.10"

     compile "com.squareup.okhttp:okhttp:1.1.1@jar"

}
